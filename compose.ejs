<%- include("partials/header") -%>

<h1>Compose</h1>

<form class="" action="/compose" method="post">

  <div class="form-group">
    <label>Title</label>
    <input type="text"   name="titleContent" class="form-control">
  </div>

  <div class="form-group">
    <label>Post</label>
    <textarea name="textArea" class="form-control" rows="6" cols="80"></textarea>
  </div>

  <button type="submit" name="button" class="btn btn-primary">Publish</button>

</form>


